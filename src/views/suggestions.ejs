<div id="suggestions-content" class="container">
    <div class="search">
        <h1 class="search-heading">"<%= search %>"?</h1>
        <h2 class="suggestions-text">Did you mean...?</h2>
    </div>

    <ul class="suggestions-list-wrapper row">
        <% suggestions.map((suggestion) => { %>
            <li class="suggestion-item col-2">
                <a href="/<%= suggestion %>"><%= suggestion %></a>
            </li>
        <% }); %>
    </ul>
    
</div>
